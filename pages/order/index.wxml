<Tabs tabs="{{tabs}}"  bindtabsItemChange="tabsItemChange">
<view class="orderMain">
  <view
    wx:for="{{orders}}"
    wx:key="order_id"
    class="orderItem"
  >
    <view class="orderNoRow">
      <view class="orderNoText">订单编号</view>
      <view class="orderNoValue">{{item.order_number}}</view>
    </view>
    <view class="orderPriceRow">
      <view class="orderNoText">订单价格</view>
      <view class="orderPriceValue">￥{{item.order_price}}</view>
    </view>
    <view class="orderTimeRow">
      <view class="orderNoText">订单日期</view>
      <view class="orderNoValue">{{item.create_time_cn}}</view>
    </view>
  </view>
</view>

</Tabs>